                }
                else {
                    alert('Password value required');
                    return false;
                }
            }
            //if (jQuery('input[name = CheckRadio]').val() != 'pass') {
            //    alert('You need to select a valid plan');
            //    return false;
            //}
            var bAdd = jQuery('input[name = BillAddress]').val();
