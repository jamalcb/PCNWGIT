@using System.Text.RegularExpressions
@model IEnumerable<OrderTables>
@{
    ViewData["Title"] = "CopyCenter";
    Layout = "~/Views/Shared/_Layout-admin.cshtml";
}
@section dataTablesCss{
    <link rel="stylesheet" href="https://cdn.datatables.net/1.13.1/css/jquery.dataTables.min.css">
}
@section dataTablesJs{
    <script src="https://cdn.datatables.net/1.13.1/js/jquery.dataTables.min.js"></script>
}
@section CustomCSS{
    <style>
        #progress-container {
            width: 90%;
            height: 26px;
            margin-top: 10px;
            border: 1px solid #ccc;
            display: none;
        }

        #progress-bar {
            height: 100%;
            width: 0;
            background-color: #3498db;
            / Set your desired color / transition: width 0.3s ease;
        }

        .ui-menu {
            z-index: 1100 !important;
        }

        .loginError {
            color: red;
        }

        .loginError2 {
            color: red;
            font-size: 13px;
            line-height: 0;
            width: 100%;
        }

        .formCls .form-control {
            margin-bottom: 5px;
        }

        #uploadError {
            padding: 8px;
            display: flex;
            align-items: center;
        }

        .bg-blue {
            background: #5c8097;
        }

        .upload-member-project-files {
            width: fit-content;
            margin: 4px;
        }

        td.newPro-btn {
            text-align: center;
        }

        .cpy-cntr-form .form-group .form-control {
            height: 32px;
        }

        #prc-total, #prc-price, #prc-size {
            text-align: right;
        }

        #loader-overlay {
            display: none;
            position: fixed;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background-color: rgba(0, 0, 0, 0.5);
            z-index: 1000;
        }

        .popup-loader {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            padding: 20px;
            border-radius: 5px;
            text-align: center;
        }

        .cst-gmail .input-group-prepend span, .input-group-append span {
            background: #f4f4f4;
            border: none;
            border-bottom: 1px solid;
            height: 45px;
            border-radius: 0;
        }

        .cst-gmail .form-control {
            border: none;
            border-bottom: 1px solid;
        }

            .cst-gmail .form-control:focus {
                box-shadow: none;
            }

        .input-group-prepend {
            margin-right: -1px;
            height: 60px;
            max-height: 54px;
        }
        /*pricing styling*/
        .rate-ul li {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 3px 14px;
            height: 32px;
        }

        .rate-head {
            position: relative;
        }

            .rate-head legend {
                width: auto;
                position: absolute;
                top: -25px;
                background: #f2f2f2;
                padding: 0 12px;
                font-weight: 600;
                color: #ff9900;
                font-size: 20px;
            }

        .rate-ul li input {
            width: 15% !important;
            margin-bottom: 0px !important;
            font-size: 14px !important;
            border: none;
            border-bottom: 1px solid #ccc;
            /*height: 1.5rem !important;*/
        }

        .rate-ul {
            border: 1px solid #f909;
        }

            .rate-ul li label {
                margin-bottom: 0px;
                font-size: 18px;
                font-weight: 500;
            }

            .rate-ul li span {
                font-weight: 600;
            }

        .header h2 {
            font-size: 20px;
            color: #666666;
            background: #fff;
        }
        /*pricing styling*/

        .color-select {
            display: none;
        }
        /* -------------- */
        #time_wrapper {
            width: fit-content;
            margin: 0 auto;
            position: relative;
            top: 150px;
            transform: scale(1.5);
        }

        #time_input {
            border: 2px solid #706e6e;
            width: fit-content;
            display: flex;
            align-items: center;
            border-radius: 0px !important;
        }

            #time_input input {
                width: 50px;
                height: auto;
                border: none;
                box-sizing: border-box;
                padding: 2px;
                text-align: center;
                color: #132c14;
                background: #f4f4f4;
            }

            #time_input label {
                display: flex;
                flex-direction: column;
                justify-content: center;
                position: relative;
                margin-bottom: 0px;
            }

        .label {
            font-size: 0.55rem;
            position: absolute;
            top: 4.5px;
        }

            .label.lbl-hrs {
                left: 20px;
            }

            .label.lbl-min {
                left: 14px;
            }

            .label.lbl-sec {
                left: 13px;
            }

        /* Chrome, Safari, Edge, Opera */
        input::-webkit-outer-spin-button,
        input::-webkit-inner-spin-button {
            -webkit-appearance: none;
            margin: 0;
        }

        /* Firefox */
        input[type="number"] {
            -moz-appearance: textfield;
        }

        #time_input,
        input {
            border-radius: 8px;
            /*font-size: 1.5rem;*/
        }

            #time_input input:focus {
                outline: 3px solid #5c8097;
                border-radius: 0;
            }

            input:focus + .label {
                color: var(--active);
            }

            input.invalid:focus {
                outline: 3px solid red;
            }

                input.invalid:focus + .label {
                    color: red;
                }

                
        .MEMloader {
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(0, 0, 0, 0.5);
            z-index: 1000;
            /*display:none;*/
        }

            .MEMloader img {
                display: block;
                margin: 0 auto;
                width: 80px;
                margin-top: 20%;
            }
    </style>
}

