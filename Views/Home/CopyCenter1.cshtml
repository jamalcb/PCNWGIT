    <script type="text/javascript">
        var idleTime = 0;
        jQuery(document).ready(function () {

            //Increment the idle time counter every minute.
            var idleInterval = setInterval(timerIncrement, 60000); // 1 minute

            //Zero the idle timer on mouse movement.
            jQuery(this).mousemove(function (e) {
                idleTime = 0;
            });
            jQuery(this).keypress(function (e) {
                idleTime = 0;
            });
        });

        function timerIncrement() {
            var autologtime;
            jQuery.ajax({
                type: "GET",
                dataType: 'json',
                url: '/GlobalMaster/GetLogOffList',
                data: {},
                async: false,
                success: function (response) {
                    jQuery('#hdnId').val(response.data[0].Id);
                    jQuery('#autoLogOff').val(response.data[0].LogOff);
                    autologtime = response.data[0].LogOff
                },
                error: function (response) {
                },
                failure: function (response) {
                }
            });

            idleTime = idleTime + 1;
            if (idleTime >= autologtime) { // 15 minutes
                jQuery.ajax({
                    type: "Get",
                    dataType: 'json',
                    url: '/Account/Logout',
                    data: { 'IsAutoLogout': true },
                    async: false,
                    success: function (response) {
                        window.location.reload();
                    },
                    error: function (response) {
                        window.location.reload();
                    },
                    failure: function (response) {
                    }
                });
            }
        }
        jQuery(document).ready(function () {
            // Get the header element
            var header = jQuery(".scroll-header");

            // Set the initial state of the header
            header.css("display", "none");

            // Add a scroll event listener
            jQuery(window).scroll(function () {
                // Get the scroll position
                var scrollPos = jQuery(window).scrollTop();

                // If the scroll position is greater than the header height, hide the header
                if (scrollPos > header.height() + 200) {
                    header.css("display", "block");
                } else {
                    // Otherwise, show the header
                    header.css("display", "none");
                }
            });
        });

    </script>

