    <style>
        .ui-menu {
            z-index: 1100 !important;
        }

        .loginError {
            color: red;
            font-size: 13px;
            line-height: 0;
        }

        .cus-HomeCopyCenter .navbar-brand {
            padding-top: 0;
            padding-bottom: 0;
            width: 320px;
        }

            .cus-HomeCopyCenter .navbar-brand img {
                max-width: 320px;
                width: auto;
            }

        .main-nav > li > a {
            font-size: 15px;
            font-weight: 400;
            letter-spacing: 0.02em;
            color: #fff;
        }

        .rate-ul li {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 3px 14px;
            height: 32px;
        }

        .rate-head {
            position: relative;
        }

            .rate-head legend {
                width: auto;
                position: absolute;
                top: -25px;
                background: #f2f2f2;
                padding: 0 12px;
                font-weight: 600;
                color: #ff9900;
                font-size: 20px;
            }

        .rate-ul li input {
            width: 15% !important;
            margin-bottom: 0px !important;
            font-size: 14px !important;
            border: none;
            border-bottom: 1px solid #ccc;
            /*height: 1.5rem !important;*/
        }

        .rate-ul {
            border: 1px solid #f909;
        }

            .rate-ul li label {
                margin-bottom: 0px;
                font-size: 18px;
            }

            .rate-ul li span {
                font-weight: bold;
            }

        .header h2 {
            font-size: 20px;
            color: #666666;
        }

        .bg-cus ul.navbar-nav li.nav-item a.nav-link {
            font-size: 18px;
            height: 80px;
            padding: 10px 15px;
        }

        .nav-item.dropdown a:hover {
            text-decoration: none;
            transition: 0.3s;
        }

        .nav-item.dropdown a {
            font-size: 18px;
            height: 80px;
            padding: 10px 15px;
            display: block;
        }

        .cus-HomeCopyCenter .navbar-brand img {
            max-width: 420px;
        }

        .lqd-custom-menu li {
            margin-bottom: 10px;
        }

            .lqd-custom-menu li a:hover {
                text-decoration: none;
            }

        .footerBottom {
            background: rgb(55 87 107);
        }

        .bottom-footer a:hover {
            text-decoration: none;
        }

        /*.logo-line::after {
                    content: "";
                    display: block;
                    border-bottom: 3px solid #5c8097;
                    width: 100%;
                    position: absolute;
                    left: 0;
                    bottom: 40px;
                }*/

        .horiz-line::before {
            content: '';
            position: absolute;
            left: 585px;
            top: 56px;
            width: 65%;
            right: 0;
            border: 1.8px solid #5c8097;
        }

        .horiz-line::after {
            content: '';
            position: absolute;
            top: 56px;
            width: 36%;
            right: 42px;
            border: 1.8px solid #5c8097;
        }

        .iconbox-inline {
            display: -webkit-box;
            display: flex;
            -webkit-box-orient: horizontal;
            -webkit-box-direction: normal;
            flex-direction: row;
            flex-wrap: wrap;
            -webkit-box-pack: start;
            justify-content: flex-start;
            text-align: left;
            align-items: baseline;
        }

            .iconbox-inline .iconbox-icon-wrap {
                flex-basis: auto;
            }

        .iconbox-xs .iconbox-icon-container {
            font-size: 18px;
            margin-right: 12px;
            color: #ff9900;
        }

        .bg-cover {
            background-size: cover !important;
        }

        .main-footer .widget-title {
            margin-bottom: 1.35em;
            font-weight: 600;
        }

        .cpy-cntr-form .form-group .form-control {
            height: 32px;
        }

        .bg-blue {
            background: #5c8097;
        }

        #loader-overlay {
            display: none;
            position: fixed;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background-color: rgba(0, 0, 0, 0.5);
            z-index: 1000;
        }

        .popup-loader {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            padding: 20px;
            border-radius: 5px;
            text-align: center;
        }
        #prc-price, #prc-total{
            text-align:right;
        }
        .scroll-header nav{
            position: fixed;
            width: 100%;
            height: 75px;
        }
        .scroll-header{
            display:none;
        }

        .scroll-cst {
            background: #37576b;
            z-index: 9;
            top: 0;
        }
        .scroll-cst .navbar-brand img{
                max-width: 340px;
            width: auto;
        }
    </style>

    <link rel="stylesheet" href="/css/custom.css">
</head>
