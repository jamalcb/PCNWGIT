
@{
    ViewData["Title"] = "UtilityDashboard";
    Layout = "~/Views/Shared/Layout-Admin-Master.cshtml";
}

<section class="content">
    <div class="container-fluid">
        <div class="block-header">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <ul class="breadcrumb breadcrumb-style ">
                        <li class="breadcrumb-item">
                            <h4 class="page-title">Reports/Data</h4>
                        </li>
                        <li class="breadcrumb-item bcrumb-1">
                            <a href="/Administration/Dashboard">
                                <i class="fas fa-home"></i> 
                            </a>
                            <a href="/Administration/UtilityDashboard">Utility</a>
                        </li>
                        <li class="breadcrumb-item active">Dashboard</li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- Widgets -->
        <div class="row">

            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                <div class="card">
                    <div class="body cus-report-body">
                        <div class="row clearfix">
                            <div class="col-xs-12 ol-sm-12 col-md-12 col-lg-12 cus-report-card mb-0">
                                <div class="panel-group" id="accordion_17" role="tablist"
                                     aria-multiselectable="true">
                                    <div class="panel panel-col-teal">
                                        <div class="panel-heading" role="tab" id="headingThree_17">
                                            <h4 class="panel-title">
                                                <a class="collapsed" role="button" data-toggle="collapse"
                                                   data-parent="#accordion_17" href="#company-db"
                                                   aria-expanded="true" aria-controls="collapseThree_17">
                                                    <i class="fas fa-cogs mr-1"></i> Utilities
                                                </a>
                                            </h4>
                                        </div>
                                        <div id="company-db" class="panel-collapse collapse show" role="tabpanel"
                                             aria-labelledby="headingThree_17">
                                            <div class="panel-body">
                                                <ul class="reports-list">
                                                    <li><a asp-controller="Administration" asp-action="CreateRole"><i class="fas fa-angle-double-right pr-2"></i>Create Role</a></li>
                                                    <li><a asp-controller="Administration" asp-action="RoleList"><i class="fas fa-angle-double-right pr-2"></i> Role List </a></li>
                                                    <li><a asp-controller="Administration" asp-action="UserList"><i class="fas fa-angle-double-right pr-2"></i> User List </a></li>
                                                    <li><a asp-controller="Administration" asp-action="PopulateS3Data"><i class="fas fa-angle-double-right pr-2"></i>Populate S3 Data</a></li>
                                                    <li><a asp-controller="Administration" asp-action="ManageStaff"><i class="fas fa-angle-double-right pr-2"></i>Manage Staff Member</a></li>
                                                    <li><a asp-controller="Administration" asp-action="Entities"><i class="fas fa-angle-double-right pr-2"></i>Manage Entity Types</a></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
