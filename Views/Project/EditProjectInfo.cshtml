@model ProjectInformation
@{
    ViewData["Title"] = "ProjectInformation";
    Layout = "~/Views/Shared/_Layout-admin.cshtml";
}
@section google {
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700">
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.css">
}
@section Scripts {
    <script src="/js/ProjectInfo.js"></script>
    <script src="/js/custom-validation.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/timepicker/1.3.5/jquery.timepicker.min.js"></script>
}
@section CustomCSS {
    <style>
        .project-info .col-md-8 {
            margin-bottom: 1rem;
        }

        .add-new-btn button {
            border: none;
            background-color: transparent;
            color: #7e8299;
        }

        .cus-border {
            border: 1px solid #ced4da;
            padding: 15px;
            border-radius: 5px;
        }

        .cus-dropdown span {
            padding: 6px 42px;
            display: inline-block;
            border-right: 1px solid #fff;
        }

        .cus-dropdown .dropdown-menu.show {
            background-color: #e4e6ef;
        }

        .cus-minus {
            background: #e3e3e3;
            background: #e3e3e3;
            border-radius: 2px;
            padding: 3px;
        }

        .project-notice h2 {
            margin-top: 30px;
        }

        .cus-refresh {
            background: #1b2c4a;
            padding: 10px;
            border-radius: 10px;
            color: #fff;
        }


        .table-border-cus {
            border: 1px solid #5c8097;
            border-radius: 4px;
        }

        .cus-label label {
            font-size: 12px;
        }

        ul#ui-id-3, ul.ui-menu.ui-widget.ui-widget-content.ui-autocomplete.ui-front {
            max-height: 200px;
            overflow-x: hidden;
            overflow-y: scroll;
        }
        /*css for fixed box and minimized project from*/
        .fix-box {
            position: sticky;
            top: 103px;
            width: 100%;
            height: auto;
            background-color: #dde3eb;
            z-index: 999;
            border: 1px solid #a9a9a9;
            padding: 8px 10px 0;
            /*  left: 0;
                                    right: 0; */
        }

        .formCls .box-space .form-control {
            height: 30px
        }

        .notice-info {
            color: #666;
            font-size: 14px;
            margin: 0;
            text-align: start;
        }

        .in-height .form-control {
            height: 30px;
            background: #f4f4f4;
            border: 1px solid #706e6e;
        }

        .mt-note {
            margin-top: 180px;
        }

        .tbl-adnda th,
        .tbl-adnda tr td {
            padding: 5px;
        }

        .edit-row label {
            color: #666;
        }

        .fix-box .newPro-btn label {
            color: #666;
            font-weight: 600;
        }

        .formCls label {
            font-size: 15px;
            font-weight: 600;
            color: #666;
        }

        .mt-project {
            /* margin-top: 135px; */
        }

        .form-control:focus {
            border-color: #5c8097;
            box-shadow: 0 0 2px #5c8097 inset, 0 0 2px #5c8097;
            outline: #5c8097 solid 1px;
        }

        .cus-textarea:focus {
            border-color: #5c8097;
            box-shadow: 0 0 2px #5c8097 inset, 0 0 2px #5c8097;
            outline: #5c8097 solid 1px;
        }
        /*css for fixed box and minimized project from*/

        :root {
            --inactive: black;
            --active: #2b662e;
        }

        #time_wrapper {
            width: fit-content;
            margin: 0 auto;
            position: relative;
            top: 150px;
            transform: scale(1.5);
        }

        #time_input {
            border: 2px solid #706e6e;
            width: fit-content;
            display: flex;
            align-items: center;
            border-radius: 0px !important;
        }

            #time_input input {
                width: 50px;
                height: auto;
                border: none;
                box-sizing: border-box;
                padding: 2px;
                text-align: center;
                color: #132c14;
                background: #f4f4f4;
            }

            #time_input label {
                display: flex;
                flex-direction: column;
                justify-content: center;
                position: relative;
                margin-bottom: 0px;
            }

        .label {
            font-size: 0.55rem;
            position: absolute;
            top: 4.5px;
        }

            .label.lbl-hrs {
                left: 20px;
            }

            .label.lbl-min {
                left: 14px;
            }

            .label.lbl-sec {
                left: 13px;
            }

        /* Chrome, Safari, Edge, Opera */
        input::-webkit-outer-spin-button,
        input::-webkit-inner-spin-button {
            -webkit-appearance: none;
            margin: 0;
        }

        /* Firefox */
        input[type="number"] {
            -moz-appearance: textfield;
        }

        #time_input,
        input {
            border-radius: 8px;
            /*font-size: 1.5rem;*/
        }

            #time_input input:focus {
                outline: 3px solid #5c8097;
                border-radius: 0;
            }

            input:focus + .label {
                color: var(--active);
            }

            input.invalid:focus {
                outline: 3px solid red;
            }

                input.invalid:focus + .label {
                    color: red;
                }

        #error {
            width: 280px;
            position: relative;
            top: 170px;
            margin: 0 auto;
            color: red;
            visibility: hidden;
        }

        .ui-autocomplete.show-all .ui-menu-item {
            display: list-item !important;
        }

        .timer {
            width: 45px;
        }

        .main-content {
            padding: 5px 0;
        }
    </style>
}
