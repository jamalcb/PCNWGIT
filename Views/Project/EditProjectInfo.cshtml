            <div class="formPage formCls">
                <input type="hidden" value="@ViewBag.ErrorMessage" id="tmpHiddenMsg" />
                <input type="hidden" value="@ViewBag.SuccessMessage" id="hdnSuccessMsg" />
                <input type="hidden" asp-for="@Model.ProjScope" />
                <input type="hidden" asp-for="@Model.IssuingOffice" />
                <input type="hidden" asp-for="@Model.ProjId" />
                <input type="hidden" asp-for="@Model.strAddenda" />
                <div class="cst-optional pro-info">
                    <div class="box-space">
                        <div class="row">
                            <div class="col-12 col-md-7">
                                <div class="border-right">
                                    <div class="row mb-1">
                                        <div class="col-lg-2 cstCol-lbl"><label> Address <span class="text-danger">*</span></label></div>
                                        <div class="col-lg-10">
                                            <div class="row">
                                                <div class="col-lg-10 mb-2">
                                                    <input type="text" asp-for="LocAddr1" class="form-control" placeholder="Address" tabindex="40" />
                                                    <input type="hidden" asp-for="Longitude">
                                                    <input type="hidden" asp-for="Latitude">
                                                </div>
                                                <div class="col-lg-2 mb-2">
                                                    <input type="text" asp-for="LocZip" class="form-control number-only" placeholder="zip code" tabindex="50" maxlength="5" />
                                                </div>
                                                <div class="col-lg-3 mb-2">
                                                    <input type="text" asp-for="LocCity" class="form-control" placeholder="city" tabindex="60" />

                                                </div>
                                                <div class="col-lg-2 mb-2">
                                                    <input type="text" asp-for="LocState" class="form-control" tabindex="65" placeholder="state">
                                                </div>
                                                <div class="col-lg-3 mb-2">
                                                    <input type="text" class="form-control" asp-for="LocAddr2" placeholder="county" tabindex="70" />
                                                    <input type="hidden" class="form-control" asp-for="CountyId" />
                                                    <a href="" data-toggle="modal" class="popClassDel" data-target="#multi-county" tabindex="75"> + Add Multiple Counties</a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="modal m-modal fade" id="multi-county" tabindex="-1" aria-labelledby="countyModalLabel" aria-hidden="true">
                                            <div class="modal-dialog modal-xl">
                                                <div class="modal-content">
                                                    <div class="modal-header blu-header">
                                                        <h5 class="modal-title" id="countyModalLabel">Add Multiple Counties</h5>
                                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close" onclick="AssignMultipleCounties()">
                                                            <span aria-hidden="true">&times;</span>
                                                        </button>
                                                    </div>
                                                    <div class="modal-body">
                                                        <!-- Checkboxes row start from here -->
                                                        <div class="d-flex justify-content-center">
                                                            @{
                                                                Model.BoolAllOr = Model.BoolAllOr == null ? false : Model.BoolAllOr;
                                                                Model.BoolAllWa = Model.BoolAllWa == null ? false : Model.BoolAllWa;
                                                            }
                                                            <div class="form-check px-4">
                                                                <input class="form-check-input classOcounties" type="checkbox" value="-1" id="AllOr" onchange="BlockCounties(this.id)" checked="@Model.BoolAllOr">
                                                                <label class="form-check-label" for="allor">
                                                                    <b>All OR Counties</b>
                                                                </label>
                                                            </div>
                                                            <div class="form-check">
                                                                <input class="form-check-input classWcounties" type="checkbox" value="-2" id="AllWa" onchange="BlockCounties(this.id, this)" checked="@Model.BoolAllWa">
                                                                <label class="form-check-label" for="allwa">
                                                                    <strong>All WA Counties</strong>
                                                                </label>
                                                            </div>
                                                        </div>
                                                        <div class="row pt-3 classCounty mx-0">
                                                            <div class="WAClass">
                                                                <p><b>WA</b></p>
                                                            </div>
                                                            <div class="ORClass">
                                                                <p><b>WA</b></p>
                                                            </div>
                                                        </div>
                                                        <div class="float-right newPro-btn">
                                                            <span class="newProbtn" style="cursor:pointer" data-dismiss="modal" onclick="AssignMultipleCounties()">OK</span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <input type="hidden" asp-for="Counties" />
                                        @if (string.IsNullOrEmpty(Model.Counties))
                                        {
                                            <div class="col-12 col-sm-2 col-md-2 col-lg-2" id="divCounty" style="display:none"><label>Counties</label></div>
                                            <div class="col-12 col-sm-10 col-md-10 col-lg-10" id="divsCounty" style="display:none"></div>
                                        }
                                        else
                                        {
