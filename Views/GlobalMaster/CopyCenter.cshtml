@{
    ViewData["Title"] = "File Storage";
    Layout = "~/Views/Shared/_Layout-SuperAdmin.cshtml";
}
@section Scripts{
    <script src="~/js/copy-center-master.js"></script>
}
@section CustomCSS{
    <style>
        .text-right .cus-pro-type .fa-plus {
            font-size: 14px !important;
        }

        a.btn.cus-pro-type {
            padding: 10px;
            background: #ff9900;
            font-weight: bold;
        }

        .cus-edit-icon {
            padding: 5px 8px;
            background: #ff9a27;
            border-radius: 5px;
        }

        .loginError {
            color: red;
            font-size: 13px;
        }

        .modl-btn.save {
            border: none;
            background: #ff9a27;
            padding: 8px 20px;
            border-radius: 5px;
        }
    </style>
}

<section class="content">
    <div class="container-fluid">
        <div class="block-header">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <ul class="breadcrumb breadcrumb-style ">
                        <li class="breadcrumb-item">
                            <h4 class="page-title">Copy Center</h4>
                        </li>
                        <li class="breadcrumb-item bcrumb-2">
                            <a href="/GlobalMaster/Dashboard">Admin</a>
                        </li>
                        <li class="breadcrumb-item active">File Storage Control</li>
                        <li class="breadcrumb-item active">Copy Center</li>
                    </ul>
                </div>
            </div>
        </div>
       
        <div class="row clearfix">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="card">
                    <div class="header">
                        <form id="frmPrintForm" action="" class="col-md-12 order-form text-left" autocomplete="off">
                            <div class="formCls">
                                <span id="message" class="loginError"></span>
                                <div class="form-group row mb-3">
                                    <label class="col-xl-2 col-lg-2 col-form-label">
                                        Copy Center<span class="text-danger">*</span>
                                    </label>
                                    <div class="col-lg-4 col-xl-4">
                                        <input type="hidden" id="Id" name="Id" value="0" />
                                        <select name="CopyCenter" id="CopyCenter" class="form-control">
                                            <option value="0">Select Copy Center Storage Period</option>
                                            <option value="1 months">1 months</option>
                                            <option value="2 months">2 months</option>
                                            <option value="3 months">3 months</option>
                                            <option value="4 months">4 months</option>
                                            <option value="5 months">5 months</option>
                                            <option value="6 months">6 months</option>
                                        </select>
                                        <span class="loginError"></span>
                                    </div>
                                    <div class="form-group col-xl-2 col-lg-2">
                                    <button id="next-btn" class="newProbtn modl-btn save" onclick="return SaveFile()">Save</button>
                                </div>
                                </div>

                                
                            </div>
                        </form>
                    </div>


                </div>
            </div>
        </div>
    </div>
</section>